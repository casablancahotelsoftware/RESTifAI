{
    "info": {
        "name": "allEntrezGeneIdsDoNotExist_FU",
        "description": "Change the request body to contain Entrez Gene IDs that are all non-existent in the database, e.g., [\"999999\", \"888888\"], to test handling of requests with no matching genes.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "[\n  \"999999\",\n  \"888888\",\n  \"672\",\n  \"1029\"\n]",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "cookies": [],
                "url": {
                    "raw": "{{baseUrl}}/ensembl/canonical-gene/entrez",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "ensembl",
                        "canonical-gene",
                        "entrez"
                    ]
                }
            },
            "event": [
                {
                    "listen": "test",
                    "script": {
                        "type": "text/javascript",
                        "exec": [
                            "pm.test('Response status code is 4xx', function () {",
                            "    pm.expect(pm.response.code || pm.response.status).to.be.within(400, 499);",
                            "});"
                        ]
                    }
                },
                {
                    "listen": "prerequest",
                    "script": {
                        "type": "text/javascript",
                        "exec": [
                            "pm.collectionVariables.set('fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST.request.headers.Content-Type', 'application/json');",
                            "pm.collectionVariables.set('fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST.request.body.[0]', '999999');",
                            "pm.collectionVariables.set('fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST.request.body.[1]', '888888');",
                            "pm.collectionVariables.set('fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST.request.body.[2]', '672');",
                            "pm.collectionVariables.set('fetchCanonicalEnsemblGeneIdByEntrezGeneIdsPOST.request.body.[3]', '1029');"
                        ]
                    }
                }
            ]
        }
    ],
    "event": [
        {
            "listen": "prerequest",
            "script": {
                "type": "text/javascript",
                "exec": [
                    "pm.environment.set('baseUrl', 'http://localhost:9000');"
                ]
            }
        }
    ]
}